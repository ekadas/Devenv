source /Library/Developer/CommandLineTools/usr/share/git-core/git-prompt.sh

alias ls='ls -G'

source ~/Devenv/tools/bash/bash_common

eval "$(/opt/homebrew/bin/brew shellenv)"

if [ -f $(brew --prefix)/etc/profile.d/bash_completion.sh ]; then
	. $(brew --prefix)/etc/profile.d/bash_completion.sh
fi

eval "$(jenv init -)"

source <(kubectl completion bash)
export BASH_SILENCE_DEPRECATION_WARNING=1

complete -C '/opt/homebrew/bin/aws_completer' aws

. <(eksctl completion bash)

# ndsctl autocomplete setup
eval $(ndsctl autocomplete:script bash)

[ -f /Users/endrek/Code/ndp.deployment-container/ndp-deployment-container.sh ] && source /Users/endrek/Code/ndp.deployment-container/ndp-deployment-container.sh

alias assume="source assume"
export PATH="/opt/homebrew/opt/libpq/bin:${KREW_ROOT:-$HOME/.krew}/bin:$PATH"
